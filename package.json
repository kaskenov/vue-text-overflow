{
  "name": "vue-text-overflow",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/vue-text-overflow.umd.js",
  "module": "./dist/vue-text-overflow.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-text-overflow.es.js",
      "require": "./dist/vue-text-overflow.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite -c ./vite.config.ts",
    "build:app": "vue-tsc -b && vite build -c ./vite.config.ts",
    "build:lib": "vue-tsc -b && vite build -c ./vite.config.lib.ts",
    "lint": "eslint"
  },
  "peerDependencies": {
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "sass-embedded": "^1.93.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  }
}
